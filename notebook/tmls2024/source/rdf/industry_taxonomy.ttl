@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix : <http://example.org/industry/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Industry rdf:type skos:Concept ;
    skos:prefLabel "Industry"@en ;
    skos:altLabel "Sector"@en ;
    skos:definition "A group of productive enterprises or organizations that produce or supply goods, services or sources of income."@en .

:Technology rdf:type skos:Concept ;
    skos:prefLabel "Technology"@en ;
    skos:altLabel "Tech"@en ;
    skos:definition "The application of scientific knowledge for practical purposes, especially in industry."@en ;
    skos:broader :Industry .

:TechMaturityScheme rdf:type skos:ConceptScheme.

:BigTech rdf:type skos:Concept ;
    skos:inScheme :TechMaturityScheme ;
    skos:prefLabel "Big tech"@en ;
    skos:altLabel "FAANG"@en, "MAANG"@en ;
    skos:broader :Technology .

:OldTech rdf:type skos:Concept ;
    skos:inScheme :TechMaturityScheme ;
    skos:prefLabel "Old tech"@en ;
    skos:broader :Technology .

:StartupTech rdf:type skos:Concept ;
    skos:inScheme :TechMaturityScheme ;
    skos:prefLabel "startup tech"@en ;
    skos:broader :Technology .

:Internet rdf:type skos:Concept ;
    skos:prefLabel "Internet"@en ;
    skos:altLabel "Web"@en, "Online"@en ;
    skos:definition "The global system of interconnected computer networks that use the Internet protocol suite (TCP/IP) to link devices worldwide."@en ;
    skos:broader :Technology .

:ECommerce rdf:type skos:Concept ;
    skos:prefLabel "E-Commerce"@en ;
    skos:altLabel "Electronic Commerce"@en, "Online Shopping"@en ;
    skos:definition "The activity of buying or selling goods and services on online services or over the Internet."@en ;
    skos:broader :Internet .

:ArtificialIntelligence rdf:type skos:Concept ;
    skos:prefLabel "Artificial Intelligence"@en ;
    skos:altLabel "AI"@en ;
    skos:definition "The theory and development of computer systems able to perform tasks that normally require human intelligence, such as visual perception, speech recognition, decision-making, and language translation."@en ;
    skos:broader :Technology .

:CloudComputing rdf:type skos:Concept ;
    skos:prefLabel "Cloud Computing"@en ;
    skos:altLabel "Cloud"@en ;
    skos:definition "The practice of using a network of remote servers hosted on the Internet to store, manage, and process data, rather than a local server or a personal computer."@en ;
    skos:broader :Technology .

:ConsumerElectronics rdf:type skos:Concept ;
    skos:prefLabel "Consumer Electronics"@en ;
    skos:altLabel "Home Electronics"@en, "Personal Electronics"@en ;
    skos:definition "Electronic equipment intended for everyday use by consumers, such as televisions, computers, and related equipment."@en ;
    skos:broader :Technology .

:DigitalDistribution rdf:type skos:Concept ;
    skos:prefLabel "Digital Distribution"@en ;
    skos:altLabel "Electronic Distribution"@en ;
    skos:definition "The delivery of digital media content such as audio, video, software, and e-books over the Internet or computer networks."@en ;
    skos:broader :Technology .

:Entertainment rdf:type skos:Concept ;
    skos:prefLabel "Entertainment"@en ;
    skos:altLabel "Amusement"@en, "Recreation"@en, "M&E"@en, "Media and Entertainment"@en ;
    skos:definition "The action of providing or being provided with amusement or enjoyment."@en ;
    skos:broader :Industry .

:SelfDrivingCars rdf:type skos:Concept ;
    skos:prefLabel "Self-Driving Cars"@en ;
    skos:altLabel "Autonomous Vehicles"@en, "Driverless Cars"@en ;
    skos:definition "Vehicles that are capable of sensing their environment and navigating without human input."@en ;
    skos:broader :Technology ;
    skos:broader :AutomotiveIndustry .

:Retail rdf:type skos:Concept ;
    skos:prefLabel "Retail"@en ;
    skos:altLabel "Retailing"@en ;
    skos:definition "The sale of goods or merchandise from a fixed location, such as a department store or bookstore, directly to the consumer in small quantities for their own use."@en ;
    skos:broader :Industry .

:Supermarket rdf:type skos:Concept ;
    skos:prefLabel "Supermarket"@en ;
    skos:altLabel "Grocery Superstore"@en ;
    skos:definition "A large self-service grocery store offering a wide variety of food and household merchandise."@en ;
    skos:broader :Retail .

:GroceryStore rdf:type skos:Concept ;
    skos:prefLabel "Grocery Store"@en ;
    skos:altLabel "Food Market"@en ;
    skos:definition "A retail store that primarily sells food products and household supplies."@en ;
    skos:broader :Retail .

:HealthFoodStore rdf:type skos:Concept ;
    skos:prefLabel "Health Food Store"@en ;
    skos:altLabel "Natural Food Store"@en, "Organic Grocery Store"@en ;
    skos:definition "A grocery store that primarily sells health foods, organic foods, local foods, and often dietary supplements."@en ;
    skos:broader :Retail .

:VideoGameIndustry rdf:type skos:Concept ;
    skos:prefLabel "Video Game Industry"@en ;
    skos:altLabel "Gaming Industry"@en ;
    skos:definition "The industry involved in the development, marketing, and monetization of video games."@en ;
    skos:broader :Entertainment ;
    skos:broader :Technology .

:AutomotiveIndustry rdf:type skos:Concept ;
    skos:prefLabel "Automotive Industry"@en ;
    skos:altLabel "Car Industry"@en, "Motor Vehicle Industry"@en ;
    skos:definition "The industry involved in the design, development, manufacturing, marketing, and selling of motor vehicles."@en ;
    skos:broader :Industry .

:EnergyStorage rdf:type skos:Concept ;
    skos:prefLabel "Energy Storage"@en ;
    skos:altLabel "Power Storage"@en ;
    skos:definition "The capture of energy produced at one time for use at a later time."@en ;
    skos:broader :Technology .

:Consulting rdf:type skos:Concept ;
    skos:prefLabel "Consulting"@en ;
    skos:altLabel "Advisory Services"@en ;
    skos:definition "The business of providing expert advice to other professionals and businesses."@en ;
    skos:broader :Industry .

:EnterpriseSoftware rdf:type skos:Concept ;
    skos:prefLabel "Enterprise Software"@en ;
    skos:altLabel "Business Software"@en ;
    skos:definition "Software used to satisfy the needs of an organization rather than individual users."@en ;
    skos:broader :Technology .

:Aerospace rdf:type skos:Concept ;
    skos:prefLabel "Aerospace"@en ;
    skos:altLabel "Aeronautics and Space"@en ;
    skos:definition "The branch of technology and industry concerned with both aviation and space flight."@en ;
    skos:broader :Industry .

:Banking rdf:type skos:Concept ;
    skos:prefLabel "Banking"@en ;
    skos:altLabel "Financial Services"@en ;
    skos:definition "The business of operating a bank, which includes activities such as lending money, accepting deposits, and providing financial services."@en ;
    skos:broader :Industry .

:CommercialBanking rdf:type skos:Concept ;
    skos:prefLabel "Commercial Banking"@en ;
    skos:altLabel "Retail banking"@en ;
    skos:broader :Banking .

:InvestmentBanking rdf:type skos:Concept ;
    skos:prefLabel "Investment Banking"@en ;
    skos:broader :Banking .

:Packaging rdf:type skos:Concept ;
    skos:prefLabel "Packaging"@en ;
    skos:altLabel "Packing"@en ;
    skos:definition "The process of enclosing or protecting products for distribution, storage, sale, and use."@en ;
    skos:broader :Industry .

:Legal rdf:type skos:Concept ;
    skos:prefLabel "Legal"@en ;
    skos:altLabel "Law firm"@en ;
    skos:definition "A lawyer walks into a bar."@en ;
    skos:broader :Industry .

:CorporateLaw rdf:type skos:Concept ;
    skos:prefLabel "Corporate law"@en ;
    skos:broader :Legal .

:Litigation rdf:type skos:Concept ;
    skos:prefLabel "Litigation"@en ;
    skos:broader :Legal .

:IntellectualProperty rdf:type skos:Concept ;
    skos:prefLabel "Intellectual property"@en ;
    skos:altLabel "IP"@en ;
    skos:broader :Legal .

